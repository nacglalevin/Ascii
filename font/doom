### punctuation mark
#\ )
eval 'export char_space=`cat`' <<"blocks"








blocks

#\!)
eval 'export char_exclamation_mark=`cat`' <<"blocks"
 _  
| | 
| | 
| | 
|_| 
(_) 


blocks

#\+)
eval 'export char_plus=`cat`' <<"blocks"

   _    
 _| |_  
|_   _| 
  |_|   



blocks

#\-)
eval 'export char_minus=`cat`' <<"blocks"


 ______  
|______| 




blocks

#\.)
eval 'export char_period=`cat`' <<"blocks"




 _  
(_) 


blocks

#\,)
eval 'export char_comma=`cat`' <<"blocks"




 _  
( ) 
|/  

blocks

#\:)
