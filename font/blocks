#!/bin/bash
export FONT_NAME="blocks"

eval 'export char_A=`cat`' <<"blocks"
 .----------------. 
| .--------------. |
| |      __      | |
| |     /  \     | |
| |    / /\ \    | |
| |   / ____ \   | |
| | _/ /    \ \_ | |
| ||____|  |____|| |
| |              | |
| '--------------' |
 '----------------' 
blocks

eval 'export char_B=`cat`' <<"blocks"
 .----------------. 
| .--------------. |
| |   ______     | |
| |  |_   _ \    | |
| |    | |_) |   | |
| |    |  __'.   | |
| |   _| |__) |  | |
| |  |_______/   | |
| |              | |
| '--------------' |
 '----------------' 
blocks

eval 'export char_C=`cat`' <<"blocks"
 .----------------. 
