blocks

### lower

export char_a="$char_A"
export char_b="$char_B"
export char_c="$char_C"
export char_d="$char_D"
export char_e="$char_E"
export char_f="$char_F"
export char_g="$char_G"
export char_h="$char_H"
export char_i="$char_I"
export char_j="$char_J"
export char_k="$char_K"
export char_l="$char_L"
export char_m="$char_M"
export char_n="$char_N"
export char_o="$char_O"
export char_p="$char_P"
export char_q="$char_Q"
export char_r="$char_R"
export char_s="$char_S"
export char_t="$char_T"
export char_u="$char_U"
export char_v="$char_V"
export char_w="$char_W"
export char_x="$char_X"
export char_y="$char_Y"
export char_z="$char_Z"

### number

